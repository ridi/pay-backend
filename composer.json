{
    "name": "ridibooks/pay",
    "description": "RIDI Pay",
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "path",
            "url": "src/Ridibooks"
        },
        {
            "type": "composer",
            "url": "https://satis.ridi.io"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "^2.8",
        "doctrine/orm": "^2.6",
        "ramsey/uuid-doctrine": "^1.4",
        "ridibooks/core": "^0.9.6",
        "ridibooks/crm": "^0.4.1",
        "sensio/framework-extra-bundle": "^5.2",
        "sentry/sentry": "^1.9.1",
        "symfony/console": "^4.1",
        "symfony/dotenv": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.1",
        "symfony/lts": "^4@dev",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.1.0",
        "symfony/phpunit-bridge": "^4.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ridibooks\\Tests\\": "tests/Ridibooks"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
